{
  "build": {
    "content": [
      {
        "files": [
          "blogs/**.md",
          "blogs/**/toc.yml",
          "random/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "blogs/solar-boiler/images/**",
          "**.jpg"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/singulinkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Making life simpler through code.",
      "_appName": "Not just an other dev blog",
      "_appFaviconPath": "images/Stroke.png",
      "_appLogoPath": "images/Stroke.png",
      "_appFooter": "<strong>Made with ❤️</strong>",
      "_copyrightFooter": "© Ben Borra. All rights reserved.",
      "_enableSearch": false,
      "_disableSideFilter": true,
      "_enableNewTab": true,
      "_disableContribution": true,
      "_disableBreadcrumb": true
    }
  }
}
